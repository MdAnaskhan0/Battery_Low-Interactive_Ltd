#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/bin/node_modules:/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/node_modules:/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules:/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/bin/node_modules:/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules/resolve/node_modules:/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/resolve@2.0.0-next.5/node_modules:/home/mdanaskhan/Battery_Low-Interactive_Ltd/Day_7/TestE-Commerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/resolve" "$@"
else
  exec node  "$basedir/../../bin/resolve" "$@"
fi
